<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Customer
 *
 * @ORM\Table(name="customer")
 * @ORM\Entity
 */
class Customer
{
    /**
     * @var string
     *
     * @ORM\Column(name="customer_title", type="string", length=45, nullable=true)
     */
    private $customerTitle;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_firstname", type="string", length=45, nullable=true)
     */
    private $customerFirstname;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_lastname", type="string", length=45, nullable=true)
     */
    private $customerLastname;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_login", type="string", length=45, nullable=true)
     */
    private $customerLogin;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_password", type="string", length=100, nullable=true)
     */
    private $customerPassword;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_email", type="string", length=50, nullable=true)
     */
    private $customerEmail;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_phone", type="string", length=45, nullable=true)
     */
    private $customerPhone;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_address", type="string", length=45, nullable=true)
     */
    private $customerAddress;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_zip", type="string", length=10, nullable=true)
     */
    private $customerZip;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_location", type="string", length=45, nullable=true)
     */
    private $customerLocation;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_title2", type="string", length=45, nullable=true)
     */
    private $customerTitle2;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_firstname2", type="string", length=45, nullable=true)
     */
    private $customerFirstname2;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_lastname2", type="string", length=45, nullable=true)
     */
    private $customerLastname2;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_address2", type="string", length=45, nullable=true)
     */
    private $customerAddress2;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_zip2", type="string", length=10, nullable=true)
     */
    private $customerZip2;

    /**
     * @var string
     *
     * @ORM\Column(name="customer_location2", type="string", length=45, nullable=true)
     */
    private $customerLocation2;

    /**
     * @var boolean
     *
     * @ORM\Column(name="customer_billingaddress", type="boolean", nullable=true)
     */
    private $customerBillingaddress;

    /**
     * @var integer
     *
     * @ORM\Column(name="customer_id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $customerId;


}

