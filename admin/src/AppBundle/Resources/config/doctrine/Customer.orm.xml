<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Customer" table="customer">
    <id name="customerId" type="integer" column="customer_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="customerTitle" type="string" column="customer_title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerFirstname" type="string" column="customer_firstname" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerLastname" type="string" column="customer_lastname" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerLogin" type="string" column="customer_login" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerPassword" type="string" column="customer_password" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerEmail" type="string" column="customer_email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerPhone" type="string" column="customer_phone" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerAddress" type="string" column="customer_address" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerZip" type="string" column="customer_zip" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerLocation" type="string" column="customer_location" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerTitle2" type="string" column="customer_title2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerFirstname2" type="string" column="customer_firstname2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerLastname2" type="string" column="customer_lastname2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerAddress2" type="string" column="customer_address2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerZip2" type="string" column="customer_zip2" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerLocation2" type="string" column="customer_location2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerBillingaddress" type="boolean" column="customer_billingaddress" nullable="true"/>
  </entity>
</doctrine-mapping>
