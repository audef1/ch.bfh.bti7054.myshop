<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Pages" table="pages">
    <id name="pageId" type="integer" column="page_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nicename" type="string" column="nicename" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="content" type="text" column="content" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lang" type="string" column="lang" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inmenu" type="boolean" column="inmenu" nullable="true"/>
    <field name="pos" type="integer" column="pos" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hidden" type="boolean" column="hidden" nullable="true"/>
    <field name="translof" type="integer" column="translof" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="parent" type="integer" column="parent" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
