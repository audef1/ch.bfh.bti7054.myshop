<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Product" table="product">
    <id name="productId" type="integer" column="product_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="productNumber" type="string" column="product_number" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productName1" type="string" column="product_name1" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productName2" type="string" column="product_name2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productNicename" type="string" column="product_nicename" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productPrice1" type="string" column="product_price1" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productPrice2" type="string" column="product_price2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productCategory" type="string" column="product_category" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productDescription" type="text" column="product_description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productDetails" type="text" column="product_details" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productFeatures" type="text" column="product_features" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productImages" type="text" column="product_images" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productOptions" type="text" column="product_options" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hidden" type="boolean" column="hidden" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="translof" type="integer" column="translof" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lang" type="string" column="lang" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
