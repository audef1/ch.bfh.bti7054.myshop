<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ProductStock" table="product_stock">
    <id name="stockId" type="integer" column="stock_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="productNumber" type="integer" column="product_number" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="stock" type="integer" column="stock" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
